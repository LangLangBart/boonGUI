<?xml version="1.0" encoding="utf-8" ?>
<!-- Unit Commands -->
<object name="unitCommandPanel" size="7 6 100% 100%" type="image" z="28">
  <object
    ghost="true"
    size="-4 100%-44 100%-4 100%-4"
    style="TranslucentPanel"
    type="text"
  />
  <repeat count="10">
    <object
      name="unitCommandButton[n]"
      hidden="true"
      style="iconButton"
      type="button"
      size="0 0 38 38"
      tooltip_style="sessionToolTipBottomBold">
      <object
        name="unitCommandIcon[n]"
        ghost="true"
        type="image"
        style="commandIcon"
      />
      <object
        name="unitCommandCount[n]"
        ghost="true"
        style="groupIconsText"
        type="text"
      />
    </object>
  </repeat>
</object>

<?xml version="1.0" encoding="utf-8" ?>
<object name="unitTrainingPanel" size="7 7 100% 100%">
  <object>
    <repeat count="27">
      <object
        name="unitTrainingButton[n]"
        hidden="true"
        style="iconButton"
        type="button"
        size="0 0 49 49"
        tooltip_style="sessionToolTipBottom">
        <object
          name="unitTrainingIcon[n]"
          type="image"
          ghost="true"
          size="3 3 46 46"
        />
        <object
          name="unitTrainingCount[n]"
          ghost="true"
          style="groupIconsText"
          type="text"
          z="20"
        />
        <action on="MouseWheelUp">OnTrainMouseWheel(1);</action>
        <action on="MouseWheelDown">OnTrainMouseWheel(-1);</action>
      </object>
    </repeat>
  </object>
</object>
